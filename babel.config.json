{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": 12
                }
            }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@atoms": "./src/atoms",
                    "@components": "./src/components",
                    "@utils": "./src/utils",
                    "@libs": "./src/libs"
                }
            }
        ]
    ]
}
